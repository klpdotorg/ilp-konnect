<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.org.klp.ilpkonnect">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".KLPApplication"
        android:allowBackup="true"
        android:icon="@drawable/ilp_logo"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:supportsRtl="true"
        android:theme="@style/KLPTheme">
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:theme="@style/KLPTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/KLPTheme" />
        <activity
            android:name=".SurveyActivity"
            android:label="@string/title_activity_survey"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/KLPTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="in.org.klp.ilpkonnect.MainActivity" />
        </activity>
        <activity
            android:name=".QuestionActivity"
            android:label="@string/title_activity_question"

            android:parentActivityName=".SurveyActivity"
            android:screenOrientation="portrait"
            android:theme="@style/KLPTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="in.org.klp.ilpkonnect.SurveyActivity" />
        </activity>
        <activity
            android:name=".UserRegistrationActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:parentActivityName=".LoginActivity"
            android:theme="@style/KLPTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="in.org.klp.ilpkonnect.LoginActivity" />
        </activity>
        <activity
            android:name=".ForgotPasswordActivity"
            android:label="@string/title_activity_forgot_password"
            android:screenOrientation="portrait"
            android:parentActivityName=".LoginActivity"
            android:theme="@style/KLPTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="in.org.klp.ilpkonnect.LoginActivity" />
        </activity>
        <activity
            android:name=".BoundarySelectionActivity"
            android:label="@string/app_name"
            android:parentActivityName=".LoginActivity"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="in.org.klp.ilpkonnect.MainDashList" />


        </activity>

        <activity
            android:name=".LoadSurveyImage"
            android:label="@string/loadSurveyImage_"
            android:parentActivityName=".ReportsActivity"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="in.org.klp.ilpkonnect.ReportsActivity" />


        </activity>


        <activity
            android:name=".SurveyDetails"

            android:screenOrientation="portrait" />
        <activity
            android:name=".SurveyTypeActivity"
            android:label="@string/surveyType"
            android:parentActivityName=".utils.AppSettings"
            android:screenOrientation="portrait">

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="in.org.klp.ilpkonnect.utils.AppSettings" />


        </activity>
        <activity
            android:name=".utils.AppSettings"
            android:screenOrientation="portrait" />

        <activity
            android:name=".MainDashList"
            android:parentActivityName=".SurveyTypeActivity"
            android:screenOrientation="portrait">


            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="in.org.klp.ilpkonnect.SurveyTypeActivity" />
        </activity>


        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".ReportsActivity"
            android:launchMode="singleInstance"
            android:noHistory="true"
            android:label="@string/app_name"

            android:screenOrientation="portrait"
            android:parentActivityName=".BoundarySelectionActivity">

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="in.org.klp.ilpkonnect.BoundarySelectionActivity" />
        </activity>

        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="755563071b87b1117422885da10c7465f9eaef1d" />

        <activity android:name=".StoriesActivity"
            android:screenOrientation="portrait"
            android:launchMode="singleInstance"
            android:parentActivityName=".BoundarySelectionActivity">

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="in.org.klp.ilpkonnect.BoundarySelectionActivity" />
        </activity>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />


        <service android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <service android:name=".MyFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>



    </application>

</manifest>